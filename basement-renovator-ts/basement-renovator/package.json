{
  "name": "basement-renovator",
  "version": "0.1.0",
  "author": "Basement Renovator",
  "description": "TBOI room editor",
  "private": true,
  "main": "public/main.js",
  "build": {
    "extraMetadata": {
      "main": "build/main.js"
    },
    "files": [
      "build/**/*"
    ]
  },
  "scripts": {
    "start": "concurrently -k \"cross-env BROWSER=none react-scripts start\" \"build-electron -d\" \"wait-on public/.build-electron-done http://localhost:3000 && electron .\"",
    "build": "build-electron && react-scripts build",
    "postinstall": "electron-builder install-app-deps",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.8.2",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.5.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/lodash": "^4.14.180",
    "@types/xml-escape": "^1.1.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "fast-xml-parser": "^4.0.7",
    "lodash": "^4.17.21",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "react-split-pane": "^0.1.92",
    "sharp": "^0.30.4",
    "transformation-matrix": "^2.11.1",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.0",
    "xml-escape": "^1.1.0"
  },
  "devDependencies": {
    "@types/sharp": "^0.30.2",
    "build-electron": "^1.0.4",
    "concurrently": "^7.0.0",
    "electron": "^18.2.0",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "node-loader": "^2.0.0",
    "path-browserify": "^1.0.1",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.3.0",
    "wait-on": "^6.0.1"
  }
}
